@model IEnumerable<DataLayer.Model.Pages>



<div class="col-md-12 col-sm-12 col-xs-12 pull-left">

    <ul class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">خانه </a></li>
        <li class="breadcrumb-item"><a href="/Archived">صفحات </a></li>
    </ul>

    <!-- last news -->
    <section class="last-news-section border-radius">
        <header><i class="icons icon-doc-2"></i>  <h3> آرشیو</h3> </header>
        <ul>
            @foreach (var item in Model)
            {
                <li>
                    <img class="border-radius" src="~/ImagesFolder/@item.ImageName">
                    <h2> <a href=""> @item.Title </a></h2>
                    <p>
                        @item.PageDescription
                    </p>
                    <span class="date-publish"><i class="fa fa-calendar"></i> انتشار : @item.CreateTime.ToShamsi() </span>
                    <a href="" class="more-link"><i class="icons icon-more"></i></a>
                </li>
            }
        </ul>
    </section>
    <!-- End last news -->
</div>

